{
    "name": "actengage/night-watch",
    "description": "The official Laravel package for the NightWatch service.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0",
        "laravel/legacy-factories": "^1.0",
        "doctrine/dbal": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\NightWatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actengage\\NightWatch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\NightWatch\\NightWatchServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpunit": "phpunit"
    }
}
